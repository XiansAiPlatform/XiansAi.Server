{
  "AuthProvider": {
    "Provider": "Auth0",
    "TenantClaimType": "https://xians.ai/tenants"
  },
  "Auth0": {
    "Domain": "https://yourdomain.auth0.com",
    "Audience": "https://your-api-audience",
    "ClientId": "your-client-id",
    "ClientSecret": "your-client-secret",
    "ManagementApi": {
      "ClientId": "your-management-api-client-id",
      "ClientSecret": "your-management-api-client-secret"
    }
  },
  "AzureB2C": {
    "TenantId": "your-tenant-id",
    "Domain": "yourdomain.onmicrosoft.com",
    "Instance": "https://yourdomain.b2clogin.com",
    "ClientId": "your-client-id",
    "ClientSecret": "your-client-secret",
    "Policy": "B2C_1_your_policy",
    "Audience": "your-audience",
    "Scope": "your-scope",
    "ManagementApi": {
      "ClientId": "your-graph-api-client-id",
      "ClientSecret": "your-graph-api-client-secret"
    }
  },
  "Keycloak": {
    "Realm": "xianAI",
    "AuthServerUrl": "https://your-keycloak-domain.com",
    "Resource": "server-api",
    "Secret": "your-client-secret",
    "Audience": "server-api, account",
    "ManagementApi": {
      "ClientId": "your-realm-admin-client",
      "ClientSecret": "your-admin-client-secret" 
    }
  }
} 