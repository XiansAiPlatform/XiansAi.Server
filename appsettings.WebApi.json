{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WebApi": {
    "Endpoints": {
      "BaseUrl": "https://localhost:5001",
      "ApiPrefix": "api/client"
    },
    "Configuration": {
      "WorkflowTimeoutMinutes": 30,
      "MaxConcurrentWorkflows": 50
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://localhost:3000"
    ]
  }
} 